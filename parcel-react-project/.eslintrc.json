{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["prettier", "eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "single"]
    }
}
